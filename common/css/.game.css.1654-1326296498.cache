@CHARSET "UTF-8";html,body{margin:0px;padding:0px;height:900px;min-width:1280px;}body{font-size:76%;text-align:left;}h1,h2{margin:0;padding:0;text-align:center;}div#container{position:relative;width:1200px;height:900px;margin:0 auto;text-align:left;overflow :hidden;}body div#container{min-height:100%;}div#navigation{margin :0 auto;position :relative;z-index:200;}div#navigation ul{margin:0;padding:0;list-style-type:none;}div#navigation li{display:inline;margin:0 0 0 1em;padding:0;}div#navigation a{text-decoration:none;}div#navigation a:hover{text-decoration:underline;}div#footer{position:absolute;bottom:0;width:100%;padding:0.5em 0;text-align:center;clear:both;min-height:20px;}div#footer a{text-decoration:underline;}div#content{padding:0;overflow:hidden;padding-bottom:50px;margin-bottom:20px;}div#destra{right:0px;position:absolute;width:150px;max-height:1000px;height:auto;z-index:200;}div#sinistra{width:750px;margin:0 auto;}.pannel{float:right;overflow:auto;height:350px;margin-top:45px;width:130px;}#info{z-index:700;position:absolute;top:23px;right:160px;background-color:white;width:200px;display:none;min-height:100px;overflow:auto;}#movemap{bottom:32px;height:auto;position:absolute;width:170px;z-index:500;}